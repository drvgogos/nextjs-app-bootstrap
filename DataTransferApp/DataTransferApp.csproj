<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net7.0-android;net7.0-ios;net7.0-maccatalyst;net7.0-windows10.0.19041.0</TargetFrameworks>
    <OutputType>Exe</OutputType>
    <RootNamespace>DataTransferApp</RootNamespace>
    <UseMaui>true</UseMaui>
    <SingleProject>true</SingleProject>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <MauiXaml Update="App.xaml" />
    <MauiXaml Update="MainPage.xaml" />
    <MauiXaml Update="Pages\BluetoothTransferPage.xaml" />
    <MauiXaml Update="Pages\USBTransferPage.xaml" />
    <MauiXaml Update="Pages\WiFiTransferPage.xaml" />
    <MauiXaml Update="Pages\QRCodeTransferPage.xaml" />
    <MauiXaml Update="Pages\WebInterfaceTransferPage.xaml" />
    <MauiXaml Update="Pages\CloudSyncTransferPage.xaml" />
    <MauiXaml Update="Pages\NFCTransferPage.xaml" />
  </ItemGroup>

</Project>
